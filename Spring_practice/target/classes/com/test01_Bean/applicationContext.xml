<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="banana" class="com.test01_Bean.MessageBeanImpl"></bean>
	<bean id="apple" class="com.test01_Bean.MessageBeanImpl">
		<constructor-arg>
			<value>apple</value>
		</constructor-arg>
		<constructor-arg value="5000"></constructor-arg>
	</bean>
	<bean id="mango" class="com.test01_Bean.MessageBeanImpl">
	<constructor-arg index="1" value="12000"></constructor-arg>
		<constructor-arg index="0" value="mango"></constructor-arg>
	</bean>
</beans>
