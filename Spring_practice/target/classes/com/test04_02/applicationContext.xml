<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="favoriteFood" name="favoriteFood" class="com.test04_02.Food">
		<property name="name" value="짜장면"></property>
		<property name="price" value="8000"></property>
	</bean>
	<bean id="unFavoriteFood" name="unFavorite" class="com.test04_02.Food">
		<property name="name" value="가지"></property>
		<property name="price" value="1000"></property>
	</bean>
	<bean id="myFood" class="com.test04_02.MyFood" autowire="byName">
	
	
	</bean>
</beans>
