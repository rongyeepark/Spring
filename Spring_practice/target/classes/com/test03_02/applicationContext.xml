<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="park" class="com.test03_02.Emp">
		<constructor-arg value="park"></constructor-arg>
		<constructor-arg value="2800000"></constructor-arg>
	</bean>
	<bean id="kim" class="com.test03_02.Emp">
		<property name="name" value="kim"></property>
		<property name="salary" value="3200000"></property>
	</bean>
</beans>
